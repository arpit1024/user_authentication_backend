{
  "name": "user-authentication",
  "version": "1.2.6",
  "description": "User Journey",
  "main": "dist/main.js",
  "scripts": {
    "build:prod": "webpack --config webpack.prod.config.js --progress",
    "start": "concurrently -r -n \"Webpack,Nodemon\" \"npm run webpack-watch\" \"npm run server-watch\"",
    "webpack-watch": "webpack --config webpack.config.js --watch --progress",
    "lint:fix": "npm run prettier && eslint \"*/**/*.ts\" --quiet --fix",
    "lint": "eslint \"*/**/*.ts\" --quiet",
    "prettier": "prettier --write \"**/*.ts\" ",
    "server-watch": "nodemon -e js dist/main.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "author": "Credflow",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.6.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
